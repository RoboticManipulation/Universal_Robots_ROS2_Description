<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro" name="robotiq_gripper">
    <!-- parameters -->
    <xacro:arg name="use_sim" default="true" />

    <!-- Import macros -->
    <xacro:include filename="$(find ur_description)/urdf/robotiq_gripper_macro.urdf.xacro" />

    <link name="world" />
    <xacro:robotiq_gripper sim="$(arg use_sim)" prefix="">
        <origin xyz="0 0 0" rpy="0 0 0" />
    </xacro:robotiq_gripper>
</robot>